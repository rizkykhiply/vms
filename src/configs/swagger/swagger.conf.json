{
    "openapi": "3.0.1",
    "info": {
        "title": "VMS API - OpenAPI",
        "version": "1.0.0",
        "description": "Documentation of VMS API version 1.0.0",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "Developer",
            "email": "developer.com"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        }
    },
    "servers": [
        {
            "url": "http://localhost:5000/api/vms",
            "description": "Whatsapp API for Local development"
        },
        {
            "url": "https://{domain}/api/vms",
            "description": "Whatsapp API for Beta & Production development",
            "variables": {
                "domain": {
                    "enum": ["beta.com", "production.com"],
                    "default": "beta"
                }
            }
        }
    ],
    "externalDocs": {
        "description": "Find out more about Swagger",
        "url": "http://swagger.io"
    },
    "tags": [
        {
            "name": "Auth"
        },
        {
            "name": "Master"
        },
        {
            "name": "Register"
        }
    ],
    "paths": {
        "/auth/login": {
            "post": {
                "tags": ["Auth"],
                "consumes": ["application/json"],
                "produces": ["application/json"],
                "requestBody": {
                    "$ref": "#/components/request/Auth/Login"
                },
                "responses": {
                    "200": {
                        "$ref": "#/components/responses/Success"
                    },
                    "201": {
                        "$ref": "#/components/responses/Created"
                    },
                    "400": {
                        "$ref": "#/components/responses/BadRequest"
                    },
                    "401": {
                        "$ref": "#/components/responses/Unauthorized"
                    },
                    "404": {
                        "$ref": "#/components/responses/NotFound"
                    },
                    "500": {
                        "$ref": "#/components/responses/InternalServerError"
                    }
                }
            }
        },
        "/auth/logout": {
            "get": {
                "tags": ["Auth"],
                "security": [{ "bearerAuth": [] }],
                "produces": ["application/json"],
                "responses": {
                    "200": {
                        "$ref": "#/components/responses/Success"
                    },
                    "201": {
                        "$ref": "#/components/responses/Created"
                    },
                    "400": {
                        "$ref": "#/components/responses/BadRequest"
                    },
                    "401": {
                        "$ref": "#/components/responses/Unauthorized"
                    },
                    "404": {
                        "$ref": "#/components/responses/NotFound"
                    },
                    "500": {
                        "$ref": "#/components/responses/InternalServerError"
                    }
                }
            }
        },
        "/master/barang": {
            "get": {
                "tags": ["Master"],
                "security": [{ "bearerAuth": [] }],
                "produces": ["application/json"],
                "responses": {
                    "200": {
                        "$ref": "#/components/responses/Success"
                    },
                    "201": {
                        "$ref": "#/components/responses/Created"
                    },
                    "400": {
                        "$ref": "#/components/responses/BadRequest"
                    },
                    "401": {
                        "$ref": "#/components/responses/Unauthorized"
                    },
                    "404": {
                        "$ref": "#/components/responses/NotFound"
                    },
                    "500": {
                        "$ref": "#/components/responses/InternalServerError"
                    }
                }
            }
        },
        "/master/kendaraan": {
            "get": {
                "tags": ["Master"],
                "security": [{ "bearerAuth": [] }],
                "produces": ["application/json"],
                "responses": {
                    "200": {
                        "$ref": "#/components/responses/Success"
                    },
                    "201": {
                        "$ref": "#/components/responses/Created"
                    },
                    "400": {
                        "$ref": "#/components/responses/BadRequest"
                    },
                    "401": {
                        "$ref": "#/components/responses/Unauthorized"
                    },
                    "404": {
                        "$ref": "#/components/responses/NotFound"
                    },
                    "500": {
                        "$ref": "#/components/responses/InternalServerError"
                    }
                }
            }
        },
        "/register/{type}": {
            "post": {
                "tags": ["Register"],
                "security": [{ "bearerAuth": [] }],
                "consumes": ["application/json"],
                "produces": ["application/json"],
                "parameters": [
                    {
                        "name": "type",
                        "in": "path",
                        "type": "integer",
                        "required": true
                    }
                ],
                "requestBody": {
                    "$ref": "#/components/request/Register"
                },
                "responses": {
                    "200": {
                        "$ref": "#/components/responses/Success"
                    },
                    "201": {
                        "$ref": "#/components/responses/Created"
                    },
                    "400": {
                        "$ref": "#/components/responses/BadRequest"
                    },
                    "401": {
                        "$ref": "#/components/responses/Unauthorized"
                    },
                    "404": {
                        "$ref": "#/components/responses/NotFound"
                    },
                    "500": {
                        "$ref": "#/components/responses/InternalServerError"
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        },
        "request": {
            "Auth": {
                "Login": {
                    "description": "Example Request Login",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/Login"
                            }
                        }
                    }
                }
            },
            "Register": {
                "description": "Example Request Register Visitor & Barang",
                "content": {
                    "application/json": {
                        "schema": {
                            "$ref": "#/components/schemas/Visitor"
                        },
                        "examples": {
                            "Barang": {
                                "$ref": "#/components/examples/Barang"
                            },
                            "Visitor": {
                                "$ref": "#/components/examples/Visitor"
                            }
                        }
                    }
                }
            }
        },
        "responses": {
            "Success": {
                "description": "Response Success",
                "content": {
                    "appication/json": {
                        "schema": {
                            "$ref": "#/components/schemas/Success"
                        }
                    }
                }
            },
            "Created": {
                "description": "Response Created",
                "content": {
                    "appication/json": {
                        "schema": {
                            "$ref": "#/components/schemas/Created"
                        }
                    }
                }
            },
            "BadRequest": {
                "description": "Response Bad Request",
                "content": {
                    "appication/json": {
                        "schema": {
                            "$ref": "#/components/schemas/BadRequest"
                        }
                    }
                }
            },
            "Unauthorized": {
                "description": "Response Unauthorized",
                "content": {
                    "appication/json": {
                        "schema": {
                            "$ref": "#/components/schemas/Unauthorized"
                        }
                    }
                }
            },
            "NotFound": {
                "description": "Response Not Found",
                "content": {
                    "appication/json": {
                        "schema": {
                            "$ref": "#/components/schemas/NotFound"
                        }
                    }
                }
            },
            "InternalServerError": {
                "description": "Response Internal Server Error",
                "content": {
                    "appication/json": {
                        "schema": {
                            "$ref": "#/components/schemas/InternalServerError"
                        }
                    }
                }
            }
        },
        "schemas": {
            "Login": {
                "required": ["username", "password"],
                "properties": {
                    "username": {
                        "type": "string",
                        "example": "admin"
                    },
                    "password": {
                        "type": "string",
                        "example": "admin"
                    }
                }
            },
            "Register": {
                "required": [
                    "idKendaraan",
                    "idBarang",
                    "waktu",
                    "namaLengkap",
                    "nik",
                    "namaInstansi",
                    "noPolisi",
                    "tujuan",
                    "imageScan",
                    "imageCam",
                    "kodeQr"
                ],
                "properties": {
                    "idKendaraan": {
                        "type": "integer",
                        "example": 1
                    },
                    "idBarang": {
                        "type": "integer",
                        "example": 1
                    },
                    "waktu": {
                        "type": "string",
                        "example": "2023-06-01 00:00:00"
                    },
                    "namaLengkap": {
                        "type": "string",
                        "example": "Budi Santoso"
                    },
                    "nik": {
                        "type": "string",
                        "example": "1122334455667788"
                    },
                    "namaInstansi": {
                        "type": "string",
                        "example": "PT. Sejahtera"
                    },
                    "noPolisi": {
                        "type": "string",
                        "example": "B1234ABC"
                    },
                    "tujuan": {
                        "type": "string",
                        "example": "Bekasi"
                    },
                    "imageScan": {
                        "type": "string"
                    },
                    "imageCam": {
                        "type": "string"
                    },
                    "kodeQr": {
                        "type": "string",
                        "example": "TRX123456"
                    }
                }
            },
            "Success": {
                "properties": {
                    "statusCode": {
                        "type": "integer",
                        "example": 200
                    },
                    "message": {
                        "type": "string",
                        "example": "Success"
                    },
                    "data": {
                        "type": "object"
                    }
                }
            },
            "Created": {
                "properties": {
                    "statusCode": {
                        "type": "integer",
                        "example": 201
                    },
                    "message": {
                        "type": "string",
                        "example": "Created"
                    }
                }
            },
            "BadRequest": {
                "properties": {
                    "statusCode": {
                        "type": "integer",
                        "example": 400
                    },
                    "message": {
                        "type": "string",
                        "example": "Bad Request"
                    }
                }
            },
            "Unauthorized": {
                "properties": {
                    "statusCode": {
                        "type": "integer",
                        "example": 401
                    },
                    "message": {
                        "type": "string",
                        "example": "Unauthorized"
                    }
                }
            },
            "NotFound": {
                "properties": {
                    "statusCode": {
                        "type": "integer",
                        "example": 404
                    },
                    "message": {
                        "type": "string",
                        "example": "Not Found"
                    }
                }
            },
            "InternalServerError": {
                "properties": {
                    "statusCode": {
                        "type": "integer",
                        "example": 500
                    },
                    "message": {
                        "type": "string",
                        "example": "Internal Server Error"
                    }
                }
            }
        },
        "examples": {
            "Barang": {
                "value": {
                    "idKendaraan": 1,
                    "idBarang": 1,
                    "waktu": "2023-06-01 00:00:00",
                    "namaLengkap": "Budi Santoso",
                    "nik": "1122334455667788",
                    "namaInstansi": "PT. Sejahtera",
                    "noPolisi": "B1234ABC",
                    "imageScan": "string",
                    "imageCam": "string",
                    "kodeQr": "TRX123456"
                }
            },
            "Visitor": {
                "value": {
                    "idKendaraan": 1,
                    "waktu": "2023-06-01 00:00:00",
                    "namaLengkap": "Budi Santoso",
                    "nik": "1122334455667788",
                    "namaInstansi": "PT. Sejahtera",
                    "noPolisi": "B1234ABC",
                    "tujuan": "Bekasi",
                    "imageScan": "string",
                    "imageCam": "string",
                    "kodeQr": "TRX123456"
                }
            }
        }
    }
}
